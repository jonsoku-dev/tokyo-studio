{
	"name": "web",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "react-router build",
		"dev": "react-router dev",
		"start": "react-router-serve ./build/server/index.js",
		"typegen": "react-router typegen",
		"typecheck": "react-router typegen && tsc",
		"lint": "biome check .",
		"format": "biome format --write .",
		"db:push": "drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"db:studio": "drizzle-kit studio",
		"notifications:process": "node -e \"require('node-fetch').default('http://localhost:5173/api/notifications/process-queue', { method: 'POST', headers: { 'Authorization': 'Bearer ' + process.env.CRON_SECRET, 'Content-Type': 'application/x-www-form-urlencoded' }, body: 'maxBatch=100&dryRun=false' }).then(r => r.json()).then(console.log).catch(console.error)\"",
		"notifications:process:dry": "node -e \"require('node-fetch').default('http://localhost:5173/api/notifications/process-queue', { method: 'POST', headers: { 'Authorization': 'Bearer ' + process.env.CRON_SECRET, 'Content-Type': 'application/x-www-form-urlencoded' }, body: 'maxBatch=100&dryRun=true' }).then(r => r.json()).then(console.log).catch(console.error)\"",
		"db:seed": "tsx scripts/seed.ts"
	},
